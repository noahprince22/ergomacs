;; haven't decided what goes here
(global-set-key (kbd "M-1") nil)
(global-set-key (kbd "M-2") nil)
(global-set-key (kbd "M-3") nil)
(global-set-key (kbd "M-4") nil)
(global-set-key (kbd "M-5") nil)
(global-set-key (kbd "M-6") nil)
(global-set-key (kbd "M-7") 'xah-previous-user-buffer)
(global-set-key (kbd "M-8") 'xah-next-user-buffer)
(global-set-key (kbd "M-9") 'xah-close-current-buffer)
(global-set-key (kbd "M-0") nil)

(global-set-key (kbd "M-o") 'other-window) ; was set face prefix key

(global-set-key (kbd "M-a") nil) ; was backward-sentence
;; (global-set-key (kbd "M-b") 'xah-close-current-buffer) ; was backward-word
;; (global-set-key (kbd "M-b") 'backward-word) ; 
(global-set-key (kbd "M-c") nil) ; was capitalize-word
(global-set-key (kbd "M-d") nil) ; was kill-word

(global-set-key (kbd "M-e") nil) ; was forward-sentence
(global-set-key (kbd "M-f") nil) ; was forward-word
(global-set-key (kbd "M-g") nil) ; was prefix for goto-char etc
(global-set-key (kbd "M-h") nil) ; was mark-paragraph
(global-set-key (kbd "M-n") nil) ; was not defined

